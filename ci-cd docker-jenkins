pipeline {
    agent any
    tools{
        maven 'maven_3_5_0'
    }
    Stages{
         Stage('build maven'){
            Steps{
                checkout([])
                sh 'mvn clean install'
            }
        }
stage('build docker image'){
      steps{
        script{
            sh 'docker build -t aliasname/imagename .'
        }
      }

    }
